{
    "name": "parcel-svelte-workspace",
    "workspaces": [
        ".yalc/@*/*",
        ".yalc/*",
        "packages/*"
    ],
    "private": true,
    "scripts": {
        "clean": "rm -r .parcel-cache packages/parcel-svelte/dist",
        "parcel": "cd packages/parcel-svelte; parcel build index.js"
    }
}
